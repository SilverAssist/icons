{
  "name": "@silverassist/icons",
  "version": "0.1.1",
  "description": "Icon library for SilverAssist - Next.js compatible React icon components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SilverAssist/icons.git"
  },
  "bugs": {
    "url": "https://github.com/SilverAssist/icons/issues"
  },
  "homepage": "https://github.com/SilverAssist/icons#readme",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,mjs,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,mjs,ts,tsx,json,md}\"",
    "prepublishOnly": "npm run build",
    "convert-svg": "node scripts/convert-svg.js",
    "generate-list": "node scripts/generate-icons-list.js",
    "bulk-convert": "node scripts/bulk-convert.js"
  },
  "keywords": [
    "react",
    "icons",
    "nextjs",
    "svg",
    "silver",
    "components"
  ],
  "author": "Miguel Colmenares <me@miguelcolmenares.com>",
  "license": "Polyform Noncommercial License 1.0.0",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks-extra": "^2.3.13",
    "prettier": "^3.6.2",
    "react": "^19.2.3",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.48.0"
  }
}
